.circle .end {
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}
.end::before {
  content: '';
  position: absolute;
  height: 30px;
  width: 3px;
  background: #eee;
  transform: rotate(-45deg);
  top: -20px;
  left: -20px;
  /* transform-origin:50% 50%; */
}
.end::after {
  content: '';
  position: absolute;
  height: 60px;
  width: 3px;
  background: #eee;
  transform: rotate(45deg);
  top: -43px;
  left: 12px;
  /* transform-origin:50% 20%; */
}
