/* 第一个 */
page {
  width: 100%;
  height: 100%;
}
.container {
  width: 100%;
  height: 100%;
  position: relative;
}
.container .title {
  position: absolute;
  top: 30px;
  color: #353535;
}
.container .desc {
  position: absolute;
  top: 60px;
  font-size: 13px;
  color: #888888;
}
.container .btn {
  border-radius: 25px;
  border: 1px solid #6078a5;
  color: #6078a5;
  width: 140px;
  height: 45px;
  line-height: 45px;
  text-align: center;
  font-size: 15px;
  margin-top: 40px;
}
/* 第二个 */
.container1 {
  background: #254a89;
  width: 100%;
  height: 100%;
  position: relative;
}
.container1 .voice {
  width: 25px;
  height: 25px;
  position: absolute;
  top: 10px;  
  right: 15px;
  border: 1px solid #d1dceb;
  padding: 5px;
  border-radius: 50%;
}
.container1 .circle {
  width: 210px;
  height: 210px;
  border-radius: 50%;
  border: 1px solid #ffffff;
  /* position: relative; */
  position: absolute;
  top: 30%;
  left: 50%;
  transform: translate(-50%, -30%);
  /* animation:moveCircle 2.5s linear infinite; */
  /* animation-play-state: running; */
  z-index: 1;
}
